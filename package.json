  "csp": {
    "default-src": "'self'",
    "font-src": "'self' https://fonts.gstatic.com https://netdna.bootstrapcdn.com/font-awesome/",
    "script-src": "'self' 'unsafe-inline' 'unsafe-eval' https://*.google.com https://*.gstatic.com https://*.mxpnl.com/ https://mixpanel.com https://js.stripe.com/v3/",
    "style-src": "'self' 'unsafe-inline' https://fonts.googleapis.com",
    "img-src": "* data: blob:",
    "connect-src": "*",
    "child-src": "*",
    "frame-src": "*",
    "frame-ancestors": "'self'",
    "report-uri": "/csp-reports",
    "worker-src": "* blob:",
    "report-to": "csp-endpoint"
  },
  "scripts": {
    "go": "yarn --ignore-scripts && yarn webpack:dll && yarn start",
    "appium": "test/node_modules/.bin/appium",
    "appium-doctor": "test/node_modules/.bin/appium-doctor",
    "appium-setup-android": "bin/appium-setup-android.sh",
    "appium-start-android": "bin/appium-start-android.sh",
    "appium-delete-android": "avdmanager delete avd --name allthingsAVD",
    "appium-authorize-ios": "test/node_modules/.bin/authorize-ios",
    "webpack:dll": "webpack --config webpack/dll/dll.config.js",
    "webpack:client": "webpack --config webpack/config.js",
    "webpack:client:legacy": "yarn webpack:client --legacy",
    "webpack:ssr": "webpack --config webpack/serverless.config.js",
    "print-env": "echo Environment: ${NODE_ENV:-\"development\"}",
    "prebuild": "yarn run clean",
    "build": "yarn build:client",
    "build:client": "npm-run-all print-env clean webpack:dll --parallel webpack:client webpack:client:legacy build:css",
    "build:ssr": "yarn webpack:ssr",
    "build:css": "bin/build-css.sh",
    "clean": "rm -rf dist public/static/css public/static/js",
    "predeploy": "yarn lint && yarn test:unit",
    "deploy": "bin/deploy.sh",
    "postinstall": "npm-run-all build:*",
    "prewatch": "mkdir -p dist/ && cd dist/ && ln -sf ../package.json && ln -sf ../public",
    "watch": "npm-run-all --parallel \"watch:* {1}\" --",
    "watch:js": "webpack/watch.js",
    "watch:css": "yarn build:css --watch &>/dev/null",
    "watch:test:unit": "yarn test:unit --watch",
    "start": "yarn watch",
    "start:standalone": "CDN_HOST_URL=\"https://app-cdn.localhost\" npm-run-all --parallel watch start:standalone:docker",
    "start:standalone:docker": "cd standalone-devenv && docker-compose down && docker-compose up",
    "lint": "yarn lint:tsc && yarn lint:tslint",
    "lint:tslint": "tslint -p tsconfig.json -t stylish",
    "lint:tsc": "tsc -p tsconfig.json --noEmit --pretty",
    "pree2e": "bin/docker-build.sh",
    "pree2e-vnc": "test -d build/node_modules || yarn pree2e",
    "e2e": "bin/e2e.sh --update --record --log mailhog,php,api,accounts,app,cockpit,nginx",
    "e2e-vnc": "bin/e2e.sh --vnc --log mailhog,php,api,accounts,app,cockpit,nginx",
    "e2e-appium": "cd test/e2e && set -a && . ./.env && SELENIUM_HOST=localhost nightwatch -c nightwatch.appium.conf.js",
    "e2e-dev": "bin/e2e-dev.sh",
    "e2e-local": "cd test/e2e && set -a && . ./.env && SELENIUM_HOST=localhost ../node_modules/.bin/nightwatch",
    "e2e-staging": "bin/e2e-staging.sh",
    "e2e-browserstack": "bin/e2e-browserstack.sh",
    "e2e-browserstack-local": "bin/e2e-browserstack-local.sh",
    "test": "yarn lint && yarn build && yarn test:unit && yarn e2e",
    "test:unit": "jest --colors",
    "test:unit:update": "yarn test:unit -u",
    "security-check": "yarn --groups=dependencies audit",
    "preversion": "yarn check --integrity && yarn security-check && yarn lint && yarn test:unit",
    "postversion": "git push --tags origin HEAD && ./bin/deploy.sh production",
    "locales:export": "node ./bin/locales-export.js",
    "locales:import": "node ./bin/locales-import.js",
    "prettier": "prettier --write",
    "svg-flags": "rm -rf public/static/img/flags && cp -R node_modules/flag-icon-css/flags/4x3 public/static/img/flags",
    "wdio": "yarn wdio:chrome && yarn wdio:firefox",
    "wdio:chrome": "PROJECT=app test/wdio/bin/wdio-dev.sh chrome",
    "wdio:firefox": "PROJECT=app test/wdio/bin/wdio-dev.sh firefox"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn prettier"
    ]
  },
  "prettier": {
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "useTabs": false
  },
  "jest": {
    "bail": true,
    "collectCoverage": true,
    "projects": [
      "<rootDir>/test/jest/jest.jsdom.config.js",
      "<rootDir>/test/jest/jest.node.config.js"
    ]
  }
}
